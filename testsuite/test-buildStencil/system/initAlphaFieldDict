FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

field "alpha.liquid";
function cylinder;
normal (2 1 -0.0);
direction (0 0 1);
//normal (1 0 -0.0);
radius 1;
origin (2 2 0.0);
centre (2 2 0.0);
invert true;


